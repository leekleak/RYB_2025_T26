#include <libpynq.h>

int main(void){
//frequency
    pynq_init();
    switchbox_set_pin(IO_AR0, SWB_PWM0); //Set PWM0 to arduino pin 0
    pwm_init(PWM0,1000*10^9); //give it a period of 1000 * 10e-9 seconds.
    pwm_set_duty_cycle(PWM0,500*10^9); //give it a duty cycle of 500 * 10e-9 seconds
    pwm_set_steps(PWM0, -1); //Set to run indefinitely
//amplitude
    pynq_init();
    switchbox_set_pin(IO_AR1, SWB_PWM1); //Set PWM0 to arduino pin 0
    pwm_init(PWM1,1000*10^9); //give it a period of 1000 * 10e-9 seconds.
    pwm_set_duty_cycle(PWM1,500*10^9); //give it a duty cycle of 500 * 10e-9 seconds
    pwm_set_steps(PWM1, -1); //Set to run indefinitely

    pwm_destroy(PWM0);
    pwm_destroy(PWM1);

}
