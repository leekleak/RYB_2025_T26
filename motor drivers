#include <libpynq.h>

int main(void){

    pynq_init();
    switchbox_set_pin(IO_AR0, SWB_PWM0); //Set PWM0 to arduino pin 0
    pwm_init(PWM0,1000); //give it a period of 1000 * 10e-9 seconds.
    pwm_set_duty_cycle(PWM0,500); //give it a duration of 500 * 10e-9 seconds
    pwm_set_steps(PWM0, 0); //Set to run indefinitely

    pwm_destroy(PWM0);

}

